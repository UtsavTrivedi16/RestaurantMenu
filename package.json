{
  "name": "restaurantmenu",
  "version": "1.0.0",
  "main": "app.js",
  "dependencies": {
    "deep-email-validator": "^0.1.21",
    "dotenv": "^16.3.1",
    "lodash": "^4.17.21",
    "mongodb": "^3.6.10"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/register": "^7.22.5",
    "chai": "^4.3.7",
    "chai-exclude": "^2.1.0",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "sinon": "^15.2.0",
    "supertest": "^6.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit",
      "pre-push": "npm run prepush"
    }
  },
  "scripts": {
    "test:unit": "mocha \"test/unit/*.test.js\"",
    "test:unit:watch": "mocha \"test/unit/*.test.js\" --watch",
    "test:all": "mocha \"test/**/*.test.js\"",
    "coverage": "nyc --reporter=lcov --reporter=text npm run test",
    "start": "node ./src/app.js",
    "lint": "eslint ./src",
    "test:unit:docker": "node run ./git/hooks/runPrePush.js",
    "precommit": "npm run lint && npm run test:unit",
    "prepush": "npm run test:unit:docker"
  },
  "nyc": {
    "exclude": [
      "test/**/*.test.js",
      ".mocharc.js"
    ]
  },
  "author": "Utsav",
  "license": "ISC",
  "directories": {
    "test": "test"
  },
  "description": ""
}
